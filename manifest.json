{
  "manifest_version": 2,
  "name": "Bing Chat Pre Prompts",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.bing.com/*"],
      "css": ["style.css"],
          "js": [
            "script.js"
          ],
    "run_at": "document_start"
    }
  ]
}
